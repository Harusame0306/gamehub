<h1>All users</h1>

<% @users.each do |user| %>
<div>
<%= link_to user_path(user) do %><%= user.username %><% end %>
<%= link_to user_path(user) do %>
  <%= attachment_image_tag user, :profile_image, fallback: "no-image.png", format: 'jpeg', size: "100x100" %>
<% end %>
<small>post games<%= user.games.count %></small>
</div>
<% end %>
