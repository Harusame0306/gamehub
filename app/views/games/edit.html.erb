<h1>Edit</h1>

<%= form_with model: @game do |f| %>
    <%= f.label :image %>
    <%= f.file_field :game_image, accept: "image/jpeg" %>

    <%= f.label :title %>
    <%= f.text_field :gametitle %>

    <%= f.label :time %>
    <%= f.text_field :gametime %>

    <%= f.label :clear %>
    <%= f.text_area :gameclear %>

    <%= f.label :review %>
    <%= f.text_area :gamebody %>

    <%= f.submit "update" %>
<% end %>

